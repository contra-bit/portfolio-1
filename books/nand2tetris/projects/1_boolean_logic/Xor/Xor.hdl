/**
 * Chip name: Xor
 * Inputs: a, b
 * Outputs: out
 * Function: If a!=b then out=1 else out=0.
 */

CHIP Xor {
	IN a, b;
	OUT out;

	PARTS:
	Nand(a=a, b=b, out=nand1);

	Not(in=a, out=nota);
	Not(in=b, out=notb);
	Nand(a=nota, b=notb, out=nand2);

	And(a=nand1, b=nand2, out=out);
}
